<h2>Hall of fame</h2>
<table class="hall">
        <tr class="hall-header">
            <th>Warrior name:</th>
            <th>Win Count:</th>
        </tr>
        {{#each allWarriors}}
            <tr class="hall-content">
                <th>{{this.name}}</th>
                <th>{{this.winCount}}</th>
            </tr>
        {{/each}}
</table>